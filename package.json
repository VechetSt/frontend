{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prod": "next build && next start",
    "lint": "next lint",
    "test": "jest",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "postinstall": "patch-package",
    "contracts:compile": "yarn contracts:compile:abi && yarn contracts:compile:v3",
    "contracts:compile:abi": "typechain --out-dir src/config/abi/types --target=ethers-v5 \"src/config/abi/*.json\"",
    "contracts:compile:v3": "typechain --target ethers-v5 --out-dir src/config/abi/types/v3 \"./node_modules/@uniswap/**/artifacts/contracts/**/*[!dbg].json\"",
    "graphql:fetch": "node fetch-schema.js",
    "graphql:generate:thegraph": "graphql-codegen --config apollo-codegen_thegraph.ts",
    "graphql:generate": "yarn graphql:generate:thegraph",
    "prepare": "yarn contracts:compile && yarn graphql:fetch && yarn graphql:generate"
  },
  "engines": {
    "node": "20.x"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^4.29.9",
    "@testing-library/react": "^13.4.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/d3": "^7.4.0",
    "@types/dom-to-image": "2.6.4",
    "@types/jest": "^29.2.5",
    "@types/lodash": "^4.14.191",
    "@types/qs": "^6.9.7",
    "@types/react-helmet": "^6.1.6",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/react-window": "^1.8.5",
    "@types/uuid": "^9.0.1",
    "eslint": "8.33.0",
    "eslint-config-next": "^13.2.4",
    "jest": "^29.4.1",
    "jest-canvas-mock": "^2.3.1",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "ts-jest": "^29.0.5",
    "ts-transform-graphql-tag": "^0.2.1",
    "typechain": "^8.1.1",
    "typescript": "^4.4.3",
    "yalc": "^1.0.0-pre.53"
  },
  "dependencies": {
    "async": "~0.2.7",
    "express": "~3.2.0",
    "jade": "~0.29.0",
    "mongodb": "~1.2.14",
    "rjs": "~2.10.0",
    "underscore": "~1.4.4",
    "vue-runtime-helpers": "^1.1.2"
  }
}
